@model Movie
@{
    ViewData["Title"] = "Movie List";
}
<h1 class="text-center my-4">@ViewData["Title"]</h1>

<div class="container">
    <table class="table table-striped table-bordered">
    <thead class="table-dark">
        <tr>
            <th>Title</th>
            <th>Category</th>
            <th>Year</th>
            <th>Director</th>
            <th>Rating</th>
            <th>Edited</th>
            <th>Lent To</th>
            <th>Notes</th>
        </tr>
    </thead>
    
    {
        <tbody>
        <td>@Model.Title</td>
        <td>@Model.Category</td>
        <td>@Model.Year</td>
        <td>@Model.Director</td>
        <td>@Model.Rating</td>
        <td>@Model.Edited</td>
        <td>@Model.Lent_to</td>
        <td>@Model.Notes</td>
        </tbody>
    }
    </table>
</div>